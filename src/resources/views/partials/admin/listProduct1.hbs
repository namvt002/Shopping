
    <h2 class="title">Products</h2>
   
         <div class="small-container">
           
                <div class="row">
                    {{#each this}}
                        <div class="col-4">
                            <img  src="{{this.productImage}}" alt=""> 
                            <h4> {{this.title}} </h4>
                            <p>{{this.price}}</p>
                            <a href="#" class="btn">Add to cart</a>
                            <a href="#" class="btn">Buy now</a>
                        </div>
                    {{/each}}
                </div>
        </div>
  
